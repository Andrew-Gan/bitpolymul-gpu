nvcc -Xcompiler -O3 -lcudart -Xcompiler '-funroll-loops -mavx2 -mpclmul -Wextra -Wall' -I/usr/local/include -I/opt/local/include -I/usr/include  -c bc.cu
gcc -O3 -funroll-loops -mavx2 -mpclmul -std=gnu99 -Wextra -Wall -I/usr/local/include -I/opt/local/include -I/usr/include  -c bitpolymul.c
gcc -O3 -funroll-loops -mavx2 -mpclmul -std=gnu99 -Wextra -Wall -I/usr/local/include -I/opt/local/include -I/usr/include  -c encode.c
gcc -O3 -funroll-loops -mavx2 -mpclmul -std=gnu99 -Wextra -Wall -I/usr/local/include -I/opt/local/include -I/usr/include  -c butterfly_net.c
gcc -O3 -funroll-loops -mavx2 -mpclmul -std=gnu99 -Wextra -Wall -I/usr/local/include -I/opt/local/include -I/usr/include  -c gf2128_cantor_iso.c
gcc -O3 -funroll-loops -mavx2 -mpclmul -std=gnu99 -Wextra -Wall -I/usr/local/include -I/opt/local/include -I/usr/include  -c btfy.c
gcc -O3 -funroll-loops -mavx2 -mpclmul -std=gnu99 -Wextra -Wall -I/usr/local/include -I/opt/local/include -I/usr/include  -c trunc_btfy_tab.c
gcc -O3 -funroll-loops -mavx2 -mpclmul -std=gnu99 -Wextra -Wall -I/usr/local/include -I/opt/local/include -I/usr/include  -c gf264_cantor_iso.c
gcc -O3 -funroll-loops -mavx2 -mpclmul -std=gnu99 -Wextra -Wall -I/usr/local/include -I/opt/local/include -I/usr/include  -c trunc_btfy_tab_64.c
g++ -O3  -mavx2 -mpclmul -fno-exceptions -fno-rtti -nostdinc++ -Wextra -Wall -I/usr/local/include -I/opt/local/include -I/usr/include  -c bitpolymul-test.cpp
nvcc  -L/usr/local/lib -L/opt/local/lib -L/usr/lib  -o bitpolymul-test bc.o bitpolymul.o encode.o butterfly_net.o gf2128_cantor_iso.o btfy.o trunc_btfy_tab.o gf264_cantor_iso.o trunc_btfy_tab_64.o bitpolymul-test.o 
rm bitpolymul-test.o
